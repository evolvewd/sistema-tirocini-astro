{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "*.config.mjs"
  ]
}